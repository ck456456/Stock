<struts>    
	<package name="s2_json" extends="struts-default" namespace="/json">
		<result-types>
			<result-type name="json" class="org.apache.struts2.json.JSONResult"/>
		</result-types>
		<interceptors>
			<interceptor name="json" class="org.apache.struts2.json.JSONInterceptor"/>
		</interceptors>
		<action name="Json01"	class="Json01"></action>
	</package>
</struts>    
